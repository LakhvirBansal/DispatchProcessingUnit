<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane fx:id="TruckMain" prefHeight="53.0" prefWidth="919.0" style="-fx-background-color: #d6eaf8;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dpu.controller.database.CustomBrokerController">
   <children>
      <TextField id="txtSearchBroker" fx:id="txtSearchCustomBroker" focusTraversable="false" layoutX="655.0" layoutY="8.0" prefHeight="32.0" prefWidth="218.0" promptText="Enter custom broker name to search..." />
      <TableView fx:id="tblCustomBroker" layoutX="0.0"
	layoutY="53.0" onMouseClicked="#tblCustomBrokerAction" prefHeight="574.0"
	prefWidth="919.0">
         <columns>
            <TableColumn fx:id="customBrokerName" maxWidth="5000.0" minWidth="101.0" prefWidth="318.0">
               <graphic>
                  <Label fx:id="lblCustomBroker" text="Custom Broker" />
               </graphic></TableColumn>
            <TableColumn fx:id="pars" prefWidth="244.0" text="PARS">
               <columns>
                  <TableColumn fx:id="parsName" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="parsPhone" prefWidth="75.0" text="Phone" />
                  <TableColumn fx:id="parsEmail" prefWidth="75.0" text="Email" />
                  <TableColumn fx:id="parsLink" prefWidth="75.0" text="Link" />
               </columns>
            </TableColumn>
            <TableColumn fx:id="paps" prefWidth="75.0" text="PAPS">
               <columns>
                  <TableColumn fx:id="papsName" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="papsPhone" prefWidth="75.0" text="Phone" />
                  <TableColumn fx:id="papsEmail" prefWidth="75.0" text="Email" />
                  <TableColumn fx:id="papsLink" prefWidth="75.0" text="Link" />
               </columns>
            </TableColumn>
         </columns>
      </TableView>
      <ImageView fx:id="btnDeleteCustomBroker" fitHeight="32.0" fitWidth="32.0" layoutX="97.0" layoutY="8.0" onMouseClicked="#btnDeleteCustomBrokerAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="com/dpu/common/xml/delete.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnEditCustomBroker" fitHeight="32.0" fitWidth="32.0" layoutX="56.0" layoutY="8.0" onMouseClicked="#btnEditCustomBrokerAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="com/dpu/common/xml/edit.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnAddCustomBroker" fitHeight="32.0" fitWidth="32.0" layoutX="14.0" layoutY="8.0" onMouseClicked="#btnAddCustomBrokerAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="com/dpu/common/xml/plus.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnGoCustomBroker" fitHeight="32.0" fitWidth="32.0" layoutX="882.0" layoutY="8.0" onMouseClicked="#btnGoCustomBrokerAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="com/dpu/common/xml/search.png" />
         </image>
      </ImageView>
   </children>
</Pane>
